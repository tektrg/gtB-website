{
  "name": "gptb-website",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "check": "astro check",
    "format": "prettier --write .",
    "lint": "eslint src/ --ext .js,.cjs,.mjs,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.cjs,.mjs,.jsx,.ts,.tsx --fix",
    "lint:all": "echo \"Note: Astro files with frontmatter cause parsing errors with standard ESLint. Use 'npm run check' for Astro validation instead.\" && npm run check",
    "seo:verify": "node AUTONOMUS/tools/seo-verify.mjs",
    "seo:publish:daily": "node AUTONOMUS/tools/publish-daily-post.mjs && npm run seo:verify",
    "generate-table": "node scripts/generate-models-table.js",
    "models:sync": "bash scripts/sync-models-api.sh"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.4.2",
    "@astrojs/rss": "^4.0.6",
    "@astrojs/sitemap": "^3.1.3",
    "@astrojs/tailwind": "^6.0.2",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "canvas-confetti": "^1.9.4",
    "clsx": "^2.1.1",
    "framer-motion": "^12.26.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.17",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/canvas-confetti": "^1.9.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "astro": "^4.10.0",
    "eslint": "^9.37.0",
    "eslint-plugin-astro": "^1.3.1",
    "lychee": "^0.2.12",
    "prettier": "^3.3.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0"
  }
}
